#!/bin/bash
# Job Name and Files (also --job-name)
#SBATCH -J heat
#Output and error (also --output, --error):
#SBATCH -o heat_task1.out
#SBATCH -e heat_task1.out
# Wall clock limit:
#SBATCH --time=00:10:00
